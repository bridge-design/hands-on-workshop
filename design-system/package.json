    "@reach/listbox": "^0.15.0",
    "core-js": "^3.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-awesome-slider": "^4.1.0",
    "react-dom": "^16.14.0",
    "styled-components": "^5.2.1"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.app",
        "preset": "iPhone 7"
      }
    }
